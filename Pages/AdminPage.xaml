<Page x:Class="MedLab.Pages.AdminPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedLab.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="650" d:DesignWidth="800"
      Title="Администратор">

    <Grid>
        <Button x:Name="ManageEmployeesButton" Content="Управление сотрудниками" HorizontalAlignment="Stretch" Margin="190,87,190,0" Height="50" VerticalAlignment="Top" Click="ManageEmployeesButton_Click"/>
        <Button x:Name="SessionsButton" Content="История входа сотрудников" HorizontalAlignment="Stretch" Margin="190,311,190,0" Height="50" VerticalAlignment="Top" Click="SessionsButton_Click"/>
        <Button x:Name="ServiceButton" Content="Управление услугами" HorizontalAlignment="Stretch" Margin="190,233,190,0" Height="50" VerticalAlignment="Top" Click="ServiceButton_Click"/>
        <Button x:Name="ManagePatients" Content="Управление пациентами" HorizontalAlignment="Stretch" Margin="190,160,190,0" Height="50" VerticalAlignment="Top" Click="ManagePatientsButton_Click"/>
    </Grid>
</Page>
