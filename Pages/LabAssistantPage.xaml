<Page x:Class="Sanatoriy.Pages.LabAssistantPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Sanatoriy.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="650" d:DesignWidth="800"
      Title="Лаборант">

    <Grid>
        <Button x:Name="AddBiomatButton" Content="Сформаировать оказание услуги" HorizontalAlignment="Stretch" Margin="190,151,190,0" Height="50" VerticalAlignment="Top" Click="AddBiomatButton_Click"/>
        <Button x:Name="ManageOrdersButton" Content="Изменение оказанием услуги" HorizontalAlignment="Stretch" Margin="190,228,190,0" Height="50" VerticalAlignment="Top" Click="ManageOrdersButton_Click"/>
        <Button x:Name="GenerateReportButton" Content="Сформировать отчёт" HorizontalAlignment="Stretch" Margin="190,306,190,0" Height="50" VerticalAlignment="Top" Click="GenerateReportButton_Click"/>
    </Grid>
</Page>
